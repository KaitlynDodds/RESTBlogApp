<% include partials/header %>

<% blogs.forEach(function(blog) { %>
    <div class="ui card">
        <h2><%= blog.title %></h2>
        <img class="ui centered large image" src="<%= blog.image %>" alt="">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href="/blogs/<%= blog._id %>">Read More!</a>
    </div>
<% }) %>    

<% include partials/footer %>